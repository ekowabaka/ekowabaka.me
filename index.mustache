<section id="about">
    <img src="assets/images/me.jpeg" />
    <p>
        I a PhD student at the Vanderbilt University, working with <a href="https://my.vanderbilt.edu/mkunda/">Dr. Maithilee Kunda</a> in the Laboratory for Airtificial Intelligence and Visual Analogical Systems. Before joining Vanderbilt, I worked as a software engineer with the National Trust Holding Company and SMSGH Solutions (now Hubtel). I earned my bachelors at the University of Ghana.
    </p>
</section>
<section id="projects">
    <h1>Projects</h1>
    <section id="projects-list">
        {{#projects}}
        <div class="project">
            <a href="{{url}}"><img src="assets/images/{{image}}" height="160" /></a>
            <a href="{{url}}" class="title">{{name}}</a>
            <p>{{description}}</p>
        </div>
        {{/projects}}
    </section>
</section>
<section id="resume">
    <h1>Resume</h1>
    <section id="resume-content">
    {{#resume}}
        <h3>{{section}}</h3>
        <section class="achievements">
        {{#achievements}}
            {{> resume_entries }}
        {{/achievements}}
        </section>
    {{/resume}}                  
    </section>                       
</section>
<section id="publications">
    <h1>Publications</h1>
    {{#publications}}
        <p>{{citation}}</p>
        <div> {{#url}}<a href="{{url}}">Paper</a>{{/url}} {{#poster}}<a href="{{poster}}">Poster</a>{{/poster}}</div>
    {{/publications}}         
</section>
